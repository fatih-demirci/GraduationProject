    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConsulConfig": {
    "Address": "http://localhost:8500",
    "ServiceAddress": "https://localhost:5001",
    "ServiceName": "IdentityService",
    "ServiceId": "Identity"
  },
